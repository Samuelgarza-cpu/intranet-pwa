    "@angular/localize": "15.0.1",
    "@angular/platform-browser": "15.0.1",
    "@angular/platform-browser-dynamic": "15.0.1",
    "@angular/router": "15.0.1",
    "@angular/service-worker": "15.0.1",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@ngrx/store": "14.3.0",
    "@profabric/angular-components": "0.1.2",
    "angular-datatables": "^15.0.0",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "4.6.1",
    "core-js": "3.25.0",
    "datatables.net": "^1.13.4",
    "datatables.net-buttons": "^2.3.6",
    "datatables.net-buttons-dt": "^2.3.6",
    "datatables.net-dt": "^1.13.4",
    "datatables.net-responsive": "^2.4.1",
    "datatables.net-responsive-dt": "^2.4.1",
    "font-awesome": "^4.7.0",
    "gatekeeper-client-sdk": "0.4.2",
    "http-server": "^14.1.1",
    "jquery": "^3.6.4",
    "jszip": "^3.10.1",
    "luxon": "3.0.3",
    "ngx-toastr": "16.0.1",
    "process": "0.11.10",
    "rxjs": "7.5.6",
    "summernote": "./summernote",
    "sweetalert2": "^11.7.2",
    "tslib": "2.4.0",
    "uuid": "8.3.2",
    "zone.js": "~0.11.8"
