{
  "name": "intranet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "ng serve",
    "build": "ng build --base-href /intranet/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update --force @angular/cli @angular/core",
    "postinstall": "ngcc",
    "pwa": "ng build --configuration production",
    "production": "ng build --configuration production --base-href /intranet/",
    "imgdigital": "ng build --configuration production"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^15.0.1",
